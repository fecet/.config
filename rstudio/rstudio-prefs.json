{
    "initial_working_directory": "~",
    "num_spaces_for_tab": 4,
    "editor_keybindings": "vim",
    "font_size_points": 16,
    "editor_theme": "Monokai",
    "pdf_previewer": "none",
    "spelling_dictionary_language": "",
    "posix_terminal_shell": "bash",
    "python_type": "conda",
    "python_version": "3.8.8",
    "python_path": "~/anaconda3/bin/python",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2",
            "HiddenTabSet"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "auto_save_on_blur": true,
    "jobs_tab_visibility": "shown"
}